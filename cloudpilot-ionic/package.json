{
  "name": "cloudpilot-ionic",
  "version": "1.5.6",
  "author": "Christian Speckner",
  "homepage": "https://cloudpilot-emu.github.io/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "yarn run gen-revision && ng run app:build",
    "gen-revision": "echo \"export const REVISION = '`git rev-parse --short HEAD`';\" > src/revision.gen.ts",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pwa-icons": "pwa-asset-generator --opaque false --favicon --scrape --icon-only -m src/manifest.webmanifest -i src/index.html assets-pwa-src/icon.html src/assets-pwa",
    "pwa-splash": "pwas-asset-generator --scrape --splash-only -m src/manifest.webmanifest -i src/index.html assets-pwa-src/splashscreen.html src/assets-pwa",
    "pwa-splash-dark": "pwa-asset-generator --scrape --splash-only --dark-mode -m src/manifest.webmanifest -i src/index.html assets-pwa-src/splashscreen.html src/assets-pwa",
    "pwa-assets": "yarn pwa-icons && yarn pwa-splash && yarn pwa-splash-dark && prettier -w --parser html src/index.html"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~12.2.7",
    "@angular/core": "~12.2.7",
    "@angular/forms": "~12.2.7",
    "@angular/platform-browser": "~12.2.7",
    "@angular/platform-browser-dynamic": "~12.2.7",
    "@angular/router": "~12.2.7",
    "@angular/service-worker": "~12.2.7",
    "@ionic/angular": "^5.8.1",
    "@types/deep-equal": "^1.0.1",
    "@types/md5": "^2.3.1",
    "@types/url-parse": "^1.4.4",
    "@types/uuid": "^8.3.1",
    "async-mutex": "^0.3.2",
    "deep-equal": "^2.0.5",
    "md5": "^2.3.0",
    "microevent.ts": "^0.2.1",
    "ngx-markdown": "^12.0.1",
    "rxjs": "~7.3.0",
    "tslib": "^2.3.1",
    "url-parse": "^1.5.3",
    "uuid": "^8.3.2",
    "whatwg-fetch": "^3.6.2",
    "worker-timers": "^7.0.40",
    "yet-another-abortcontroller-polyfill": "^0.0.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.7",
    "@angular/cli": "~12.2.7",
    "@angular/compiler": "~12.2.7",
    "@angular/compiler-cli": "~12.2.7",
    "@angular/language-service": "~12.2.7",
    "@ionic/angular-toolkit": "^4.0.0",
    "@types/emscripten": "^1.39.5",
    "@types/jasmine": "~3.9.1",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^16.9.6",
    "codelyzer": "^6.0.2",
    "import-sort-style-eslint": "^6.0.0",
    "jasmine-core": "~3.9.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "prettier": "^2.4.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "protractor": "~7.0.0",
    "pwa-asset-generator": "^4.3.9",
    "raw-loader": "^4.0.2",
    "ts-node": "~10.2.1",
    "tslint": "~6.1.0",
    "typescript": "~4.3.0"
  },
  "description": "Cloudpilot PWA",
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "singleQuote": true,
    "parser": "typescript"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "eslint",
      "parser": "typescript"
    }
  }
}
